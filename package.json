{
  "name": "boilerplate-vue-ts",
  "version": "1.0.0",
  "description": "Vue.js boilerplate with TypeScript",
  "main": "main.js",
  "scripts": {
    "build": "npx webpack --config ./config/webpack.config.js --progress",
    "test:watch": "npx jest --watchAll --verbose",
    "test:coverage": "npx jest --watchAll --verbose --coverage",
    "dev": "npx webpack serve --config ./config/webpack.config.dev.js --progress --open chrome",
    "sb": "npx start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cadenzah/boilerplate-vue-ts.git"
  },
  "keywords": [
    "vuejs",
    "vue",
    "javascript",
    "webpack",
    "storybook",
    "tdd",
    "jest",
    "vue-test-utils"
  ],
  "author": "cadenzah <cadenzah93@gmail.com> (https://github.com/cadenzah)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cadenzah/boilerplate-vue-ts/issues"
  },
  "homepage": "https://github.com/cadenzah/boilerplate-vue-ts#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-env": "^7.12.1",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-viewport": "^6.1.20",
    "@storybook/vue": "^6.1.20",
    "@vue/test-utils": "^1.1.1",
    "@webpack-cli/serve": "^1.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.9.0",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^5.0.0",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^11.0.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.5",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.5.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
